.action_bar {
    bottom: 0;
    left: 0;
    padding: 0.3rem 0.1rem 0.4rem 0.1rem;
    position: fixed;
    width: 100%;
    border-top: 0.1rem solid #51486f;
    background: #242031;
    z-index: 9001;
    font-weight: 600;
    text-align: center;
}

@media screen and (min-width: 1120px) {
    .action_bar {
        text-align: right;
        padding: 10px 10px 12px 10px;
    }
}

.action_bar span {
    display: none;
}

@media screen and (min-width: 480px) {
    .action_bar span {
        display: inline;
    }
}

.action_bar a {
    padding-left: 15px;
    text-align: center;
}

.action_bar .call {
    display: none;
}

.action_bar .call a {
    color: white;
}

@media screen and (min-width: 1120px) {
    .action_bar .call {
        display: block;
        font-size: 27px;
        color: #fff;
        position: absolute;
        left: 30px;
        top: 11px;
        font-weight: 300;
    }

    .action_bar .call strong {
        font-weight: bold;
        color: white;
    }
}

.action_bar .donate {
    display: none;
    margin-top: 0;
    margin: 0 5px 0px 15px;
    border: 0;
    padding: 0 15px 0 15px;
    height: 30px;
    box-sizing: border-box;
    font-size: 16px;
    font-weight: bold;
    line-height: 27px;
    width: 120px;
    color: #fbf4ff;
    font-family: 'Open Sans', 'open-sans', Arial, sans-serif;
    cursor: pointer;
    text-decoration: none;
    text-transform: uppercase;
    text-align: center;
    border-radius: 25px;
    background: #5f5f84;
    /* Old browsers */
}

@media screen and (min-width: 480px) {
    .action_bar .donate {
        display: inline-block;
    }
}

.action_bar button.social {
    margin-top: 2px;
}

nav {
    position: absolute;
    left: 0px;
    width: 100%;
    background: #242031;
    border-bottom: 0.1rem solid #51486f;
    box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.5);
    text-align: center;
    padding: .5rem 0 .8rem 0;
    z-index: 9001;
}

nav a {
    color: rgba(251, 244, 255, 0.9);
    font-weight: 600;
    font-size: 1.3rem;
}

@media screen and (min-width: 480px) {
    nav a {
        font-size: 1.8rem;
    }
}

nav ul li {
    display: inline-block;
}

nav ul li:before {
    content: '\00a0\2022\00a0\00a0';
    color: #fbf4ff;
    font-size: 1.5rem;
}

nav ul li:first-child:before {
    content: '';
}

body .sp_fb_small, body .sp_tw_small {
    display: inline-block;
    height: 30px;
    vertical-align: bottom;
    margin: 0px 5px;
}

body .sp_fb_small a, body .sp_tw_small a {
    border: 0;
    border-radius: 7.142857143em;
    box-sizing: border-box;
    color: white;
    cursor: pointer;
    display: inline-block;
    font-family: 'Open Sans', 'open-sans', Arial, sans-serif;
    font-weight: bold;
    height: 30px;
    width: 120px;
    line-height: 2;
    position: relative;
    text-decoration: none;
    text-transform: uppercase;
    font-size: 16px;
}

body .sp_fb_small a::before, body .sp_tw_small a::before {
    position: absolute;
    display: block;
    left: 45px;
    top: 0px;
    height: 30px;
    line-height: 30px;
}

@media screen and (max-width: 480px) {
    div.share .sp_fb_small, div.share .sp_tw_small {
        margin: 0px;
    }
}

body .sp_fb_small a {
    background-color: #3b5a95;
    left: 1rem;
    position: relative;
}

body .sp_fb_small a::before {
    background-position: -50% 0;
    width: 1.8rem;
    height: 1.8rem;
    background-image: url(<%= image_url 'share-icons.png' %>);
    background-repeat: no-repeat;
    background-size: auto 1.8rem;
}

body .sp_fb_small a::after {
    content: 'SHARE';
}

body .sp_tw_small a {
    background-color: #17a1ce;
    left: 1rem;
    position: relative;
}

body .sp_tw_small a::before {
    background-position: 0 0;
    width: 1.8rem;
    height: 1.8rem;
    background-image: url(<%= image_url 'share-icons.png' %>);
    background-repeat: no-repeat;
    background-size: auto 1.8rem;
}

body .sp_tw_small a::after {
    content: 'TWEET';
}

.video-wrapper {
    width: 60rem;
    margin: 0 auto;
    max-width: 100%;
    border: 4px solid #353535;
}

.video-container {
    height: 0;
    margin: 0 auto;
    max-width: 66rem;
    padding-bottom: 56.25%;
    padding-top: 3rem;
    position: relative;
    overflow: hidden;
}

.video-container iframe, .video-container object, .video-container embed {
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

iframe.map {
    display: block;
    height: 400px;
    margin: 0 auto;
    max-width: 90%;
    width: 768px;
}

.departments {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
}

.more {
    bottom: 0;
    background-image: linear-gradient(to bottom, transparent, #060607 75%);
    color: #bcc7e4;
    font-family: 'Open Sans', 'open-sans', Arial, sans-serif;
    font-size: 2.2rem;
    height: 12.7rem;
    left: 0;
    padding: 7rem 0 0;
    position: absolute;
    right: 0;
    text-align: center;
    text-transform: uppercase;
    transition: background-image 300ms;
}

.more:link, .more:visited {
    text-decoration: none;
}

.more:hover, .more:active {
    color: #fbf4ff;
    text-decoration: underline;
}

.more:hover::before, .more:active::before {
    color: #fbf4ff;
    text-decoration: none;
}

.more::before {
    display: inline-block;
    color: rgba(188, 199, 228, 0.5);
    content: '▼';
    margin-right: 1rem;
}

.candidates {
    padding: 10%;
}

.candidates .candidates_table {
    border-radius: 5px;
    max-width: 768px;
    margin: 60px auto 0 auto;
    color: #fbf4ff;
    padding: 1rem 0 0;
    text-align: center;
    background: #232330;
}

.clickable {
    text-decoration: none;
}

.candidate_shot:hover {
    background: #212140;
    border-radius: 30px;
}
